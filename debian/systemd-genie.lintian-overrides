# Must be setuid
elevated-privileges usr/bin/genie 6755 root/root

# Python should be more usual
unusual-interpreter /usr/bin/python [usr/lib/genie/genie]
unusual-interpreter /usr/bin/python [usr/lib/genie/runinwsl]
